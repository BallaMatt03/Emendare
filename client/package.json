{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=8.3.0"
  },
  "license": "GPL-3.0",
  "scripts": {
    "start": "node app.js",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "cypress:open": "npx cypress open",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/eslint {src,public,cypress}/**/*.js",
    "format": "prettier --write {src,public,cypress}/**/*.{js,jsx,css,scss,json,md}",
    "format:ci": "prettier --list-different {src,public,cypress}/**/*.{js,jsx,css,scss,json,md}",
    "codecov": "codecov -t 187fc664-9fb9-4a79-9064-c7e6d96fc023"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "npm run format",
      "npm run lint",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "ie >= 8"
  ],
  "dependencies": {
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "diff-match-patch": "^1.0.4",
    "express": "^4.16.4",
    "express-sslify": "^1.2.0",
    "lodash": "^4.17.11",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "socket.io-client": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "codecov": "^3.1.0",
    "cypress": "^3.1.4",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-react-app": "^3.0.6",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-cypress": "^2.2.0",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "eslint-plugin-sonarjs": "^0.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3"
  }
}
